{
	"keywords" : [
		"typo3 dummy"
	],
	"name" : "sj/typo3cms-distribution",
	"type" : "project",
	"description" : "A dummy installation prepared for migrating contents from 'DAM'- to 'Media'-extension.",
	"license" : "GPL-3.0+",
	"minimum-stability" : "dev",
	"scripts" : {
		"post-install-cmd" : [
			"[ -x vendor/bin/satis-github-publisher.sh ] && vendor/bin/satis-github-publisher.sh"
		],
		"post-update-cmd" : [
			"[ -x vendor/bin/satis-github-publisher.sh ] && vendor/bin/satis-github-publisher.sh"
		]
	},
	"require" : {
		"php" : ">=5.4",
		"typo3/ext-dam_falmigration" : "dev-master"
	},
	"require-dev" : {
		"sj/satis-github-scripts" : "dev-master"
	},
	"repositories" : [{
			"type" : "composer",
			"url" : "http://sjorek.github.io/typo3cms-installers",
			"name" : "sj/typo3cms-installers"
		}, {
			"type" : "package",
			"name" : "typo3/ext-dam_falmigration",
			"package" : {
				"name" : "typo3/ext-dam_falmigration",
				"type" : "typo3cms-extension",
				"version" : "master",
				"license" : "GPL-2.0+",
				"dist" : {
					"url" : "https://github.com/fnagel/t3ext-dam_falmigration/archive/master.zip",
					"type" : "zip"
				},
				"source" : {
					"type" : "git",
					"url" : "git@github.com:fnagel/t3ext-dam_falmigration.git",
					"reference" : "master"
				},
				"require" : {
					"typo3/cms" : "dev-master",
					"typo3/ext-media" : "dev-master"
				}
			}
		}, {
			"type" : "package",
			"name" : "typo3/ext-media",
			"package" : {
				"name" : "typo3/ext-media",
				"type" : "typo3cms-extension",
				"version" : "master",
				"license" : "GPL-2.0+",
				"dist" : {
					"url" : "https://git.typo3.org/TYPO3v4/Extensions/media.git/snapshot/master.tar.gz",
					"type" : "tar"
				},
				"source" : {
					"type" : "git",
					"url" : "git://git.typo3.org/TYPO3v4/Extensions/media.git",
					"reference" : "master"
				},
				"require" : {
					"typo3/cms" : "dev-master",
					"typo3/ext-vidi" : "dev-master"
				}
			}
		}, {
			"type" : "package",
			"name" : "typo3/ext-vidi",
			"package" : {
				"name" : "typo3/ext-vidi",
				"type" : "typo3cms-extension",
				"version" : "master",
				"license" : "GPL-2.0+",
				"dist" : {
					"url" : "https://git.typo3.org/TYPO3CMS/Extensions/vidi.git/snapshot/master.tar.gz",
					"type" : "tar"
				},
				"source" : {
					"type" : "git",
					"url" : "git://git.typo3.org/TYPO3CMS/Extensions/vidi.git",
					"reference" : "master"
				},
				"require" : {
					"typo3/cms" : "dev-master"
				}
			}
		}
	],
	"homepage" : "http://sjorek.github.io/typo3cms-distribution",
	"support" : {
		"source" : "https://github.com/sjorek/typo3cms-distribution"
	}
}